{
    "audio": {
        "channel": 1,
        "sample_rate": 44100
    },
    "fft": {
        "clipping_threshold": 1,
        "max_allowed_silent_frames": 20,
        "min_allowed_spectrum_length": 120,
        "window_size": 510,
        "window_overwrap_rate": 0.25
    },
    "dataset":{
        "org": {
            "dirname": "data/original",
            "ext": ""
        },
        "norm": {
            "dirname": "data/normalized",
            "type": "directory",
            "ext": ".wav"
        },
        "fft": {
            "dirname": "data/fft",
            "ext": ".npz"
        }
    }
}
